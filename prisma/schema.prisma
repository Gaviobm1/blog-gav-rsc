generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DATABASE_URL_DIRECT")
}

model Posts {
  id         Int       @id @default(autoincrement())
  post     String   @db.Text
  href       String   @db.VarChar(500)
  published DateTime @default(dbgenerated("CURRENT_DATE")) @db.Date

  @@map("posts")
}
